{
  "name": "sofistik-tools",
  "description": "A superset of commands to improve SOFiSTiK workflow",
  "repository": "https://github.com/asiloisad/pulsar-sofistik-tools",
  "keywords": [
    "sofistik",
    "tools"
  ],
  "version": "1.4.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "consumedServices": {
    "tree-view": {
      "versions": {
        "^1.0.0": "consumeTreeView"
      }
    },
    "open-external": {
      "versions": {
        "^1.0.0": "consumeOpenExternalService"
      }
    },
    "sofistik.keywords": {
      "versions": {
        "^2.0.0": "consumeSofistikKeywords"
      }
    },
    "pdf-viewer": {
      "versions": {
        "^1.0.0": "consumePdfViewer"
      }
    }
  },
  "dependencies": {
    "glob": "^11.1.0",
    "minimatch": "^5.1.6",
    "pulsar-select-list": "^1.0.0"
  },
  "configSchema": {
    "envPath": {
      "order": 1,
      "title": "SOFiSTiK installation path",
      "description": "Path to the SOFiSTiK installation folder.",
      "type": "string",
      "default": "C:\\Program Files\\SOFiSTiK"
    }
  }
}
