'atom-text-editor[data-grammar="source sofistik"]':
  'f5'      : 'sofistik-tools:calculation-wps'
  'ctrl-f5' : 'sofistik-tools:calculation-wps-immediately'
  'shift-f5': 'sofistik-tools:calculation-wps-current'
  'alt-f5'  : 'sofistik-tools:calculation-sps-immediately'
  'f6'      : 'sofistik-tools:current-help'
  'alt-f6'  : 'sofistik-tools:separately-help'
  'f7'      : 'sofistik-tools:open-animator'
  'alt-f7'  : 'sofistik-tools:open-animator-2018'
  'f8'      : 'sofistik-tools:open-wingraf'
  'ctrl-f8' : 'sofistik-tools:open-result-viewer'
  'f9'      : 'sofistik-tools:open-report'

'.sofistik-tools.help-list atom-text-editor[mini]':
  'enter'    : 'select-list:open-in'
  'alt-enter': 'select-list:open-ex'
